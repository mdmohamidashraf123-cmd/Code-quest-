Happy=[]
Stressed=[]
Focused=[]
Chill=[]
mood = input("Enter the mood you are in: ")
h,sa,f,c=0,0,0,0
if mood=="Happy":
    s=input("Enter the song name: ")
    Happy.append(s)
    h+=1

if mood=="Stressed":
    s=input("Enter the song name: ")
    Stressed.append(s)
    sa+=1

if mood=="Focused":
    s=input("Enter the song name: ")
    Focused.append(s)
    f+=1

if mood=="Chill":
    s=input("Enter the song name: ")
    Chill.append(s)
    c+=1
l=[h,sa,f,c]
a=l.max()
l.remove(a)
b=l.max()
l.remove(b)
c=l.max()
if a==h and b==sa and c==f:
 print("Top 3 moods you are in today are:Happy, Stressed, Focused")
elif a==h and b==sa and c==c:
 print("Top 3 moods you are in today are:Happy, Stressed, Chill")
elif a==h and b==f and c==c:
    print("Top 3 moods you are in today are:Happy, Focused, Chill")
elif a==sa and b==f and c==c:
    print("Top 3 moods you are in today are:Stressed, Focused, Chill")
elif a==sa and b==h and c==f:
    print("Top 3 moods you are in today are:Stressed, Happy, Focused")
elif a==f and b==c and c==h:
    print("Top 3 moods you are in today are:Focused, Chill, Happy")
else:
    print("Top 3 moods you are in today are:Focused, Chill, Stressed")
